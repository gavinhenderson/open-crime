{
  "name": "open-crime-graphql",
  "version": "0.0.1",
  "description": "A graphql api for data.police.uk",
  "main": "index.js",
  "scripts": {
    "start": "npm run docs; node index.js",
    "dev": "./node_modules/.bin/nodemon index.js --config nodemon.json",
    "production": "node index.js --production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "npm run schema; ./node_modules/.bin/graphdoc -s ./schema.graphql -o ./docs/ --force",
    "schema": "node ./graphql"
  },
  "author": "Gavin Henderson",
  "license": "ISC",
  "dependencies": {
    "@2fd/graphdoc": "^2.4.0",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "graphql-import": "^0.6.0",
    "grunt": "^1.0.3",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-watch": "^1.1.0",
    "nodemon": "^1.18.3",
    "request": "^2.87.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "grunt-exec": "^3.0.0"
  }
}
